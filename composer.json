{
    "name": "xver/symfony-baseapp-bundle",
    "type": "symfony-bundle",
    "description": "Provides base for symfony app",
    "keywords": [],
    "license": "AGPL-3.0-or-later",
    "require": {
        "php": "^8.1",
        "symfony/security-bundle": "6.2.*",
        "symfony/form": "6.2.*",
        "symfony/uid": "6.2.*",
        "symfony/validator": "6.2.*",
        "xver/domain-bundle": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "vimeo/psalm": "^5.9"
    },
    "autoload": {
        "psr-4": { "xVer\\Symfony\\Bundle\\BaseAppBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "xVer\\Symfony\\Bundle\\BaseAppBundle\\Tests\\": "tests/" }
    },
    "repositories":[
        {
            "type": "vcs",
            "url": "https://github.com/sgurdiel/php-domain-bundle.git"
        }
    ],
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
